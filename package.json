{
  "name": "code-header",
  "displayName": "code-header",
  "description": "Automatically adds file header with author and date",
  "version": "0.0.1",
  "repository": {
  "type": "git",
  "url": "https://github.com/KingofPandas1/vscode-code-header.git"
},
  "engines": {
    "vscode": "^1.105.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:code-header.insertHeader"
],
"main": "./out/extension.js",
"contributes": {
    "commands": [
        {
            "command": "code-header.insertHeader",
            "title": "Insert File Header"
        }
    ]
},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.105.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@typescript-eslint/eslint-plugin": "^8.45.0",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.36.0",
    "typescript": "^5.9.3",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2"
  }
}
